{
    "name": "interpid/fpdf",
    "description": "Fpdf Addons",
    "type": "library",
    "authors": [
        {
            "name": "Interpid",
            "email": "office@interpid.eu"
        }
    ],
    "require": {
        "ext-mbstring": "*",
        "setasign/fpdf": "^1.8"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpmetrics/phpmetrics": "^2.4",
        "interpid/redmine": "^1.0",
        "kbsali/redmine-api": "dev-master",
        "vlucas/phpdotenv": "^4.1"
    },
    "autoload": {
        "classmap": [
            "library/interpid"
        ],
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Interpid\\PdfLib\\Tests\\": "tests/",
            "Interpid\\PdfExamples\\": "library/interpid/PdfExamples/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://gitlab-ci:YiVedS2532Vk5fXnVyxz@gitlab.interpid.eu/interpid/redmine.git"
        },
        {
            "type": "git",
            "url": "https://github.com/klodoma/php-redmine-api.git"
        }
    ]
}
