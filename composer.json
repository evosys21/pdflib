{
    "name": "evosys21/pdflib",
    "description": "Fpdf, TCPDF, tFpdf - Advanced Multicell and Table",
    "license": [
        "MIT"
    ],
    "type": "library",
    "authors": [
        {
            "name": "EvoSys21",
            "email": "office@evosys21.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "ext-json": "*",
        "setasign/fpdf": "^1.8.6",
        "tecnickcom/tcpdf": "^6.7.0",
        "setasign/tfpdf": "^1.33.0",
        "phpunit/phpunit": "^9.0",
        "phpmetrics/phpmetrics": "^2.4",
        "vlucas/phpdotenv": "^4.1",
        "ext-fileinfo": "*",
        "symfony/process": "^5.3"
    },
    "autoload": {
        "psr-4": {
            "EvoSys21\\PdfLib\\": "src/",
            "EvoSys21\\PdfLib\\Examples\\": "examples/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EvoSys21\\PdfLib\\Dev\\": "dev/",
            "EvoSys21\\PdfLib\\Tests\\": "tests/"
        }
    }
}
